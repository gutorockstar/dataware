<?php

$toolbar = new \Admin\Entity\Toolbar();
$toolbar->setDisableToolbarActions(array(
    Admin\Entity\Toolbar::TB_ACTION_PRINT,
    Admin\Entity\Toolbar::TB_ACTION_CLONE,
    Admin\Entity\Toolbar::TB_ACTION_UNIFY
));

$form = $this->form;
$form->prepare();

$editForm = $this->form()->openTag($form);              
$editForm .= $this->formCollection($form);
$editForm .= $this->form()->closeTag();
$panel = new \Admin\Entity\Panel('Editar país', '');

$view = new \Admin\Entity\View();
$view->setToolbar($toolbar);
$view->setPanel($panel);

echo $this->ViewHelper($view);

?>